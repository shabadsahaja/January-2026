from collections import deque
from typing import List, Optional
class Solution:
    def zigzagLevelOrder(self, root: Optional[TreeNode]) -> List[List[int]]:
        
        if not root:
            return []
        ans = []
        q = deque([root])
        level_count = 0
        while q:
            level = [] 
            for _ in range(len(q)):
                node = q.popleft()
                level.append(node.val)  
                if node.left:
                    q.append(node.left)
                if node.right:
                    q.append(node.right)
            if level_count % 2 == 1:
                ans.append(level[::-1])
            else:
                ans.append(level)
            level_count += 1
        return ans
