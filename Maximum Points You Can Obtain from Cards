def maxScore(cardPoints,k):
    n=len(cardPoints)
    left=0
    right=k-1
    Sum=sum(cardPoints[0:k])
    maxSum=Sum
    j=n-1
    for _ in range(k):
        Sum-=cardPoints[right]
        right-=1
        Sum+=cardPoints[j]
        j-=1
        maxSum=max(maxSum,Sum)
    return maxSum

    
        
