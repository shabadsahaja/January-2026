class Solution:
    def minCostClimbingStairs(self, cost: List[int]) -> int:
        n = len(cost)
        dp = [-1] * (n + 1)
        def solve(i):
            if i <= 1:
                return 0
            if dp[i] != -1:
                return dp[i]
            dp[i] = min(solve(i - 1) + cost[i - 1],solve(i - 2) + cost[i - 2])
            return dp[i]
        return solve(n)
