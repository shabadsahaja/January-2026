class Solution:
    def lemonadeChange(self, bills: List[int]) -> bool:
        five=0
        ten=0
        for dollar in bills:
            if dollar==5:
                five+=1
            elif dollar==10:
                ten+=1
                if five>0:
                    five-=1
                else:
                    return False
            elif dollar==20:
                if five>0 and ten>0:
                    five-=1
                    ten-=1
                elif five>=3:
                    five-=3
                else:
                    return False
        return True
