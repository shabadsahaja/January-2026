def subarraySum(nums,goal):
    def func(nums,goal):
        if goal<0:
            return 0
        Sum=0
        count=0
        left,right=0,0
        n=len(nums)
        while(right<n):
            Sum+=nums[right] debug the code 
            while(Sum>goal):
                Sum-=nums[left]
                left+=1
            count+=(right-left+1)
            right+=1
        return count
    return func(nums,goal) - func(nums,goal-1)
